// Dummy shaders for platforms that do not support terrain.
// These are not used at runtime but only here to avoid PhyreAssetProcessor
// errors when it processes terrain files for unsupported platforms.


float4 DummyVP()
{			
	return float4(1,1,1,1);	
}

float4 DummyFP() : COLOR0
{
	return float4(0,0,0,0);
}